{
  "name": "happydogs-backend",
  "description": "Nest TypeScript HappyDogs Prototype Backend",
  "license": "MIT",
  "author": "Dominik Dorfstetter",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": "^12.14.1",
    "npm": "^6.13.4",
    "yarn": "^1.21.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:prod": "npm run build",
    "lint": "eslint --ext .js,.ts src/**",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf dist",
    "preinstall": "node ./tools/check-yarn.js",
    "start": "ts-node src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "test": "jest --config jest.config.js",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "test:watch": "npm run test -- --watch",
    "tpl-repo": "tools/merge-with-repository-template.sh",
    "tpl-repo:check": "tools/merge-with-repository-template.sh -- check",
    "tpl-repo:merge": "tools/merge-with-repository-template.sh -- merge",
    "orm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:diff": "npm run orm -- migration:generate --name",
    "migrations:run": "npm run orm -- migration:run",
    "migrations:revert": "npm run orm -- migration:revert",
    "seeds:new": "ts-node src/configs/seed-generator.ts generate",
    "seeds:run": "npm run orm -- migration:run --connection 'seeding'",
    "seeds:revert": "npm run orm -- migration:revert --connection 'seeding'"
  },
  "dependencies": {
    "@nestjs/common": "~7.6.13",
    "@nestjs/config": "~0.6.3",
    "@nestjs/core": "~7.6.13",
    "@nestjs/platform-express": "~7.6.13",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "~7.1.5",
    "@z-brain/typeorm-postgres-camelcase-naming-strategy": "~1.0.18",
    "csurf": "^1.11.0",
    "dotenv": "~8.2.0",
    "express": "^4.17.1",
    "lodash": "~4.17.21",
    "module-alias": "~2.2.2",
    "nest-keycloak-connect": "^1.4.3",
    "pg": "~8.5.1",
    "pluralize": "~8.0.0",
    "prettier": "^2.2.1",
    "ramda": "~0.27.1",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "~0.2.31",
    "uuid": "^8.3.2",
    "yargs": "~16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "~7.5.6",
    "@nestjs/schematics": "~7.2.8",
    "@nestjs/testing": "~7.6.13",
    "@types/express": "~4.17.1",
    "@types/jest": "~26.0.20",
    "@types/lodash": "~4.14.168",
    "@types/module-alias": "~2.0.0",
    "@types/node": "~12.20.4",
    "@types/pluralize": "~0.0.29",
    "@types/ramda": "~0.27.38",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "~8.0.17",
    "ts-node": "~8.10.2",
    "tsc-watch": "~4.1.0",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "post-merge": "IS_YARN=true yarn install"
    }
  }
}
